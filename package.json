{
    "name": "@framerjs/component-importer",
    "version": "0.0.22",
    "main": "./build/index.js",
    "types": "./build/index.d.ts",
    "repository": "git@github.com:framer/ComponentImporter.git",
    "author": "Dan-el Khen <danelkhen@gmail.com>",
    "license": "MIT",
    "scripts": {
        "build": "rm -rf ./build && yarn && yarn tsc && chmod +x ./build/cli.js",
        "cli": "node build/cli",
        "test": "jest",
        "test:blueprint": "yarn build && yarn cli generate --config design-systems/blueprint/importer.config.json",
        "test:material": "yarn build && yarn cli --packageName @material-ui/core --tsconfig design-systems/material-ui/repo/packages/material-ui/tsconfig.json --index design-systems/material-ui/repo/packages/material-ui/src/index.d.ts --out design-systems/material-ui/out/",
        "test:grommet": "yarn build && yarn cli --packageName grommet --index design-systems/grommet/repo/src/js/index.d.ts --out design-systems/grommet/out",
        "test:ant-design": "yarn build && yarn cli --packageName antd --tsconfig design-systems/ant-design/repo/tsconfig.json --index design-systems/ant-design/repo/components/index.tsx --out design-systems/ant-design/out/",
        "prepublishOnly": "yarn test && yarn build"
    },
    "dependencies": {
        "command-line-args": "^5.1.1",
        "fs-extra": "^8.0.1",
        "glob": "^7.1.4",
        "prettier": "^1.17.1",
        "typescript": "^3.6.2"
    },
    "devDependencies": {
        "@types/command-line-args": "^5.0.0",
        "@types/fs-extra": "^7.0.0",
        "@types/glob": "^7.1.1",
        "@types/jest": "^24.0.15",
        "@types/prettier": "^1.16.3",
        "@types/react": "^16.8.23",
        "jest": "^24.8.0",
        "react": "^16.8.6",
        "ts-jest": "^24.0.2"
    },
    "files": [
        "/build",
        "README.md",
        "tsconfig.json"
    ],
    "bin": {
        "component-importer": "./build/cli.js"
    }
}
